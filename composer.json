{
    "name": "arokettu/encryptor",
    "description": "CLI File Encryptor",
    "homepage": "https://sandfox.dev/php/encryptor.html",
    "keywords": [
        "php", "encryption", "sodium"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "sandfox@sandfox.me",
            "homepage": "https://sandfox.me/",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/sandfox/php-encryptor",
        "issues": "https://gitlab.com/sandfox/php-encryptor/-/issues"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "platform-check": true,
        "sort-packages": true
    },
    "bin": [
        "bin/encryptor"
    ],
    "autoload": {
        "psr-4": {
            "Arokettu\\Encryptor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\Encryptor\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2 | ^8.0",
        "ext-sodium": "*",
        "sandfoxme/bencode": "^1.5 | ^2.5",
        "symfony/console": "^4.0 | ^5.0",
        "symfony/polyfill-php80": "^1.18"
    },
    "require-dev": {
        "composer-runtime-api": "^2",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "indeyets/pake": "^1.99",
        "knplabs/github-api": "^3.0",
        "nyholm/psr7": "^1.4",
        "phpcompatibility/php-compatibility": "*",
        "phpunit/phpunit": "^8.5 | ^9.5",
        "secondtruth/phar-compiler": "^1.1",
        "slevomat/coding-standard": "*",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/http-client": "^4 | ^5"
    }
}
