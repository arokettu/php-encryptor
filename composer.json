{
    "name": "sandfoxme/encryptor",
    "description": "CLI File Encryptor",
    "keywords": [
        "php", "encryption", "sodium"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "sandfox@sandfox.me"
        }
    ],
    "bin": [
        "bin/encryptor"
    ],
    "autoload": {
        "psr-4": {
            "SandFox\\Encryptor\\": "src"
        },
        "files": [
            "src/polyfill.php"
        ]
    },
    "require": {
        "php": ">= 7.2",
        "ext-sodium": "*",
        "sandfoxme/bencode": "^1.3",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "secondtruth/phar-compiler": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    }
}
