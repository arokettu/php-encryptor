{
    "name": "arokettu/encryptor",
    "description": "CLI File Encryptor",
    "homepage": "https://sandfox.dev/php/encryptor.html",
    "keywords": [
        "php", "encryption", "sodium"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Smirnov",
            "email": "sandfox@sandfox.me"
        }
    ],
    "bin": [
        "bin/encryptor"
    ],
    "autoload": {
        "psr-4": {
            "SandFox\\Encryptor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-sodium": "*",
        "sandfoxme/bencode": "^1.3 || ^2.0",
        "symfony/console": "^4.0 || ^5.0",
        "symfony/polyfill-php80": "^1.18"
    },
    "require-dev": {
        "indeyets/pake": "^1.99",
        "knplabs/github-api": "^2.11",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "^8.2",
        "secondtruth/phar-compiler": "^1.1",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "platform-check": true,
        "sort-packages": true
    }
}
